ormState.data = response.data.ballot\r\n          // for(var item of response.data.ballot){\r\n\r\n          // }\r\n          console.log(this.formState.data)\r\n          console.log(typeof(this.formState.data[0]))\r\n        }\r\n        if(response.data.result == 'error1'){\r\n           message.destroy()\r\n           message.warning('请检查登录')\r\n        }\r\n         if(response.data.result == 'error2'){\r\n           message.destroy()\r\n           message.warning('没有找到对应投票项目')\r\n        }\r\n      })\r\n    },\r\n  }\r\n\r\n});\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.ant-pro-page-header-search {\r\n  text-align: center;\r\n  margin-bottom: 16px;\r\n}\r\n.step-form-style-desc {\r\n  padding: 0 56px;\r\n  color: rgba(0,0,0,.45);\r\n\r\n  h3 {\r\n    margin: 0 0 12px;\r\n    color: rgba(0,0,0,.45);\r\n    font-size: 16px;\r\n    line-height: 32px;\r\n  }\r\n\r\n  h4 {\r\n    margin: 0 0 4px;\r\n    color: rgba(0,0,0,.45);\r\n    font-size: 14px;\r\n    line-height: 22px;\r\n  }\r\n\r\n  p {\r\n    margin-top: 0;\r\n    margin-bottom: 12px;\r\n    line-height: 22px;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}