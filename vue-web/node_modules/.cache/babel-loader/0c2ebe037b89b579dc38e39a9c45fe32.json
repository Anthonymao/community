{"ast":null,"code":"import emitter from '@/utils/bus';\nimport { message } from 'ant-design-vue';\nimport { defineComponent, reactive } from 'vue';\nexport default defineComponent({\n  inject: ['reload'],\n\n  setup() {\n    const formState = reactive({\n      ID: '',\n      title: '',\n      startTime: '',\n      endTime: '',\n      choices: [],\n      signature: '',\n      result: [],\n      type: '',\n      vote: false\n    });\n    const formStateWe = reactive({\n      addr: '',\n      title: '',\n      content: '',\n      endTime: '',\n      weight: '',\n      PKlist: [],\n      PK: '',\n      rawballot: '',\n      signature: '',\n      choice: '',\n      vote: false\n    });\n\n    let validateSignature = async (_rule, value) => {\n      if (value === '') {\n        return Promise.reject('请根据要求操作');\n      } else {\n        return Promise.resolve();\n      }\n    };\n\n    const rules = {\n      signature: [{\n        required: true,\n        validator: validateSignature,\n        trigger: 'change'\n      }]\n    };\n    return {\n      formState,\n      rules,\n      validateSignature,\n      formStateWe\n    };\n  },\n\n  created() {\n    emitter.on(\"info1\", msg => {\n      // console.log(\"sdsdsdsdssssss\", msg)\n      if (msg.type == 'we') {\n        this.formStateWe.addr = msg.addr, this.formStateWe.title = msg.voteinfo[0], this.formStateWe.content = msg.voteinfo[2], this.formStateWe.endTime = msg.voteinfo[1], this.formStateWe.weight = msg.weight, this.formStateWe.PKlist = msg.PK, this.formStateWe.vote = true;\n      } else {\n        this.formState.ID = msg.ID, this.formState.title = msg.title, this.formState.endTime = msg.ddl, this.formState.choices = msg.choice, this.formState.vote = true, this.formState.type = msg.type; // this.formState.type = true\n      }\n    });\n  },\n\n  methods: {\n    beforeUpload(file) {\n      // console.log('上传前校验--文件类型', file)\n      // this.fileList = [file]\n      // console.log('选择了文件beforeUpload', this.fileList)\n      this.read(file);\n      return false;\n    },\n\n    read(f) {\n      const reader = new FileReader();\n      reader.readAsText(f, 'UTF-8');\n\n      reader.onload = fileReader => {\n        const fileData = fileReader.target.result;\n        var parafile = JSON.parse(fileData); // console.log(para, typeof (para));\n        // console.log(JSON.parse(reader.result));\n        // 上面的两个输出相同\n\n        var index = this.formStateWe.PKlist.indexOf(parafile.pk);\n        console.log(\"indexshism\", index);\n\n        if (index == -1) {\n          message.destroy();\n          message.warning(\"文件上传错误，不是当前投票活动参数\");\n        }\n\n        var para = new Object();\n        para.P = parafile.P;\n        para.Q = parafile.Q;\n        para.G = parafile.G;\n        var PK = generatepub(para, this.formStateWe.PKlist, this.formStateWe.PKlist.length);\n        console.log(\"系统公钥建立：\", PK);\n        this.formStateWe.PK = PK; // C[i] = encrypt(para, r, PK, m[i], 0)\n\n        var mi = this.formStateWe.choice * this.formStateWe.weight;\n        this.formStateWe.rawballot = JSON.stringify(encrypt(para, parafile.r, PK, mi, 0));\n        var ecdsa_pubs = new Object();\n        ecdsa_pubs.x = parafile.ecdsa_pk_x;\n        ecdsa_pubs.y = parafile.ecdsa_pk_y;\n        this.formStateWe.signature = sign(this.formStateWe.rawballot, parafile.ecdsa_sk, ecdsa_pubs); // signs[i] = sign(jsonC[i], man[i].ecdsa_sk, ecdsa_pubs[i])\n\n        console.log(\"选票和签名\", this.formStateWe.rawballot, this.formStateWe.signature);\n      };\n    },\n\n    prevStep() {\n      //  emitter.off('info')\n      this.$emit('prevStep');\n      this.reload();\n    },\n\n    nextStep() {\n      var res = [];\n\n      if (this.formState.signature !== '' && this.formState.signature.length == this.formState.signature.split(\" \").join(\"\").length && this.formState.pattern) {\n        res.push({\n          ID: this.formState.ID,\n          ballot: this.formState.pattern,\n          signature: this.formState.signature,\n          vote: 'avec'\n        });\n        console.log(\"构造数据\", res);\n        emitter.emit('upload', res);\n        this.$emit('nextStep');\n      } else if (this.formState.signature !== '' && this.formState.signature.length == this.formState.signature.split(\" \").join(\"\").length && this.formState.result.length) {\n        res.push({\n          ID: this.formState.ID,\n          ballot: this.formState.result,\n          signature: this.formState.signature,\n          vote: 'avec'\n        });\n        console.log(\"构造数据\", res);\n        emitter.emit('upload', res);\n        this.$emit('nextStep');\n      }\n    },\n\n    nextStepWe() {\n      console.log(this.formStateWe.PK); // this.$emit('nextStep')\n\n      if (this.formStateWe.choice) {\n        if (this.formStateWe.signature == '') {\n          message.destroy();\n          message.warning(\"请点击并上传本地加密信息文件\");\n        } // console.log(JSON.parse(this.formStateWe.rawballot))\n        // this.$emit('nextStep')\n\n\n        this.addweballot();\n      }\n    },\n\n    addweballot() {\n      this.$http.post('http://192.168.5.42:8888/voter/add_we_ballot', {\n        name: sessionStorage.getItem('user'),\n        password: sessionStorage.getItem('pass'),\n        uid: sessionStorage.getItem('uid'),\n        ballotstr: this.formStateWe.rawballot,\n        signature: JSON.stringify(this.formStateWe.signature),\n        weight: this.formStateWe.weight,\n        addr: this.formStateWe.addr\n      }).then(response => {\n        console.log(response);\n\n        if (response.data.result == 'success') {\n          emitter.emit('change', response.data);\n          console.log(\"jieguo:\", response);\n          emitter.emit('info1', response.data);\n          this.$emit('nextStep');\n        }\n\n        if (response.data.result == 'error1') {\n          message.destroy();\n          message.warning('请检查登录');\n        }\n\n        if (response.data.result == 'error2') {\n          message.destroy();\n          message.warning('没有找到对应投票项目');\n        }\n\n        if (response.data.result == 'error3') {\n          emitter.emit('change', \"fail\"); // this.$emit('nextStep')\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAuHA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AACA,eAAeD,eAAe,CAAC;EAC7BE,MAAM,EAAE,CAAC,QAAD,CADqB;;EAE7BC,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIH,QAAQ,CAAC;MACzBI,EAAE,EAAE,EADqB;MAEzBC,KAAK,EAAE,EAFkB;MAGzBC,SAAS,EAAE,EAHc;MAIzBC,OAAO,EAAE,EAJgB;MAKzBC,OAAO,EAAE,EALgB;MAMzBC,SAAS,EAAE,EANc;MAOzBC,MAAM,EAAE,EAPiB;MAQzBC,IAAI,EAAE,EARmB;MASzBC,IAAI,EAAE;IATmB,CAAD,CAA1B;IAYA,MAAMC,WAAU,GAAIb,QAAQ,CAAC;MAC3Bc,IAAI,EAAE,EADqB;MAE3BT,KAAK,EAAE,EAFoB;MAG3BU,OAAO,EAAE,EAHkB;MAI3BR,OAAO,EAAE,EAJkB;MAK3BS,MAAM,EAAE,EALmB;MAM3BC,MAAM,EAAC,EANoB;MAO3BC,EAAE,EAAE,EAPuB;MAQ3BC,SAAS,EAAE,EARgB;MAS3BV,SAAS,EAAE,EATgB;MAU3BW,MAAM,EAAE,EAVmB;MAW3BR,IAAI,EAAE;IAXqB,CAAD,CAA5B;;IAaA,IAAIS,iBAAgB,GAAI,OAAOC,KAAP,EAAcC,KAAd,KAAwB;MAC9C,IAAIA,KAAI,KAAM,EAAd,EAAkB;QAChB,OAAOC,OAAO,CAACC,MAAR,CAAe,SAAf,CAAP;MACF,CAFA,MAGK;QACH,OAAOD,OAAO,CAACE,OAAR,EAAP;MACF;IACF,CAPA;;IAQA,MAAMC,KAAI,GAAI;MACZlB,SAAS,EAAE,CAAC;QACVmB,QAAQ,EAAE,IADA;QAEVC,SAAS,EAAER,iBAFD;QAGVS,OAAO,EAAE;MAHC,CAAD;IADC,CAAd;IAQA,OAAO;MACL3B,SADK;MAELwB,KAFK;MAGLN,iBAHK;MAILR;IAJK,CAAP;EAMD,CAlD4B;;EAmD7BkB,OAAO,GAAG;IACRlC,OAAO,CAACmC,EAAR,CAAW,OAAX,EAAoBC,GAAE,IAAK;MACzB;MACA,IAAIA,GAAG,CAACtB,IAAJ,IAAY,IAAhB,EAAsB;QACpB,KAAKE,WAAL,CAAiBC,IAAjB,GAAwBmB,GAAG,CAACnB,IAA5B,EACE,KAAKD,WAAL,CAAiBR,KAAjB,GAAyB4B,GAAG,CAACC,QAAJ,CAAa,CAAb,CAD3B,EAEE,KAAKrB,WAAL,CAAiBE,OAAjB,GAA2BkB,GAAG,CAACC,QAAJ,CAAa,CAAb,CAF7B,EAGE,KAAKrB,WAAL,CAAiBN,OAAjB,GAA2B0B,GAAG,CAACC,QAAJ,CAAa,CAAb,CAH7B,EAIE,KAAKrB,WAAL,CAAiBG,MAAjB,GAA0BiB,GAAG,CAACjB,MAJhC,EAKE,KAAKH,WAAL,CAAiBI,MAAjB,GAA0BgB,GAAG,CAACf,EALhC,EAOE,KAAKL,WAAL,CAAiBD,IAAjB,GAAwB,IAP1B;MAQF,CATA,MAUK;QACH,KAAKT,SAAL,CAAeC,EAAf,GAAoB6B,GAAG,CAAC7B,EAAxB,EACE,KAAKD,SAAL,CAAeE,KAAf,GAAuB4B,GAAG,CAAC5B,KAD7B,EAEE,KAAKF