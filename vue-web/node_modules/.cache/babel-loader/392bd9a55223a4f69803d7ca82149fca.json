e = reactive({\r\n      title: '',\r\n      startTime:'',\r\n      endTime:'',\r\n      choices: [],\r\n    });\r\n\r\n\r\n    return {\r\n      formState,\r\n    };\r\n  },\r\n\r\n  created(){\r\n    emitter.on(\"Res\",msg=>{\r\n      this.formState.title = msg[0].Title\r\n      this.formState.startTime = msg[0].startTime\r\n      this.formState.endTime = msg[0].endTime\r\n      this.formState.choices = msg[1]\r\n      console.log(\"这个是传过来的数据111\",msg[1])\r\n      console.log('zheshi',this.title)\r\n    });\r\n  },\r\n\r\n   methods:{\r\n     prevStep(){\r\n      this.$emit('prevStep')\r\n    },\r\n    nextStep(){\r\n      this.$emit('nextStep')\r\n    }\r\n  }\r\n\r\n});\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .information {\r\n    line-height: 22px;\r\n    // margin-top: 24px;\r\n    padding: 0px 0px;\r\n    background-color: #fafafa;\r\n    // max-width: 500px;\r\n    // margin: 4px auto 2px ;\r\n\r\n    .ant-row:not(:last-child) {\r\n      margin-bottom: 24px;\r\n    }\r\n  }\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}