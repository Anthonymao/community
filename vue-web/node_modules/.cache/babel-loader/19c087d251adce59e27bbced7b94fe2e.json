sole.log(this.formState.ID)\r\n        // this.$emit('nextStep')\r\n      }\r\n    },\r\n    send(){\r\n      this.$http.post('http://192.168.5.42:8888/voter/create', {\r\n        name:sessionStorage.getItem('user'),\r\n        password:sessionStorage.getItem('pass'),\r\n        title:this.formState.title,\r\n        startTime:this.formState.startTime,\r\n        endTime:this.formState.endTime,\r\n        choice:this.formState.choices,\r\n      }).then((response) =>{\r\n        console.log(response)\r\n        if(response.data.result == 'success'){\r\n          \r\n          emitter.emit('ResID',response.data.ID)\r\n          this.$emit('nextStep')\r\n        }\r\n      })\r\n    }\r\n  }\r\n\r\n});\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.step-form-style-desc {\r\n  padding: 0 56px;\r\n  color: rgba(0,0,0,.45);\r\n\r\n  h3 {\r\n    margin: 0 0 12px;\r\n    color: rgba(0,0,0,.45);\r\n    font-size: 16px;\r\n    line-height: 32px;\r\n  }\r\n\r\n  h4 {\r\n    margin: 0 0 4px;\r\n    color: rgba(0,0,0,.45);\r\n    font-size: 14px;\r\n    line-height: 22px;\r\n  }\r\n\r\n  p {\r\n    margin-top: 0;\r\n    margin-bottom: 12px;\r\n    line-height: 22px;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}