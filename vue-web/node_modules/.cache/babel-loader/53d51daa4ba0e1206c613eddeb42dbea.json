ern) {\n        res.push({\n          ID: this.formState.ID,\n          ballot: this.formState.pattern,\n          signature: this.formState.signature,\n          vote: 'avec'\n        });\n        console.log(\"构造数据\", res);\n        emitter.emit('upload', res);\n        this.$emit('nextStep');\n      } else if (this.formState.signature !== '' && this.formState.signature.length == this.formState.signature.split(\" \").join(\"\").length && this.formState.result.length) {\n        res.push({\n          ID: this.formState.ID,\n          ballot: this.formState.result,\n          signature: this.formState.signature,\n          vote: 'avec'\n        });\n        console.log(\"构造数据\", res);\n        emitter.emit('upload', res);\n        this.$emit('nextStep');\n      }\n    },\n\n    generatepub() {\n      var para = new Object();\n      para.P = this.formStateWe.P;\n      para.Q = this.formStateWe.Q;\n      para.G = this.formStateWe.G;\n      var enc = generatekey(para);\n      enc.r = Math.round(Math.random() * 100000 + 2).toString();\n      var spub = new Object();\n      spub.x = enc.ecdsa_pk_x;\n      spub.y = enc.ecdsa_pk_y;\n      this.formStateWe.spub = this.downloadTxt(enc);\n    },\n\n    downloadTxt(enc) {\n      let str = JSON.stringify(enc);\n      let strData = new Blob([str], {\n        type: 'text/plain;charset=utf-8'\n      });\n      saveAs(strData, \"权重密钥信息.txt\");\n      message.destroy();\n      message.success(\"请注意保存权重密钥文件\");\n    },\n\n    nextStepWe() {\n      console.log(\"sdsdsdsdsd\", enc); // this.$emit('nextStep')\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAuGA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,SAAQC,MAAR,QAAqB,YAArB;AACA,eAAeH,eAAe,CAAC;EAC7BI,MAAM,EAAC,CAAC,QAAD,CADsB;;EAE7BC,KAAK,GAAG;IACL,MAAMC,SAAQ,GAAIL,QAAQ,CAAC;MAC1BM,EAAE,EAAC,EADuB;MAE1BC,KAAK,EAAE,EAFmB;MAG1BC,SAAS,EAAC,EAHgB;MAI1BC,OAAO,EAAC,EAJkB;MAK1BC,OAAO,EAAE,EALiB;MAM1BC,SAAS,EAAC,EANgB;MAO1BC,MAAM,EAA