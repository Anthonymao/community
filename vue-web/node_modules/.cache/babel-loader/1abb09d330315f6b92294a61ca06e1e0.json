{"ast":null,"code":"import emitter from '@/utils/bus';\nimport { defineComponent, reactive } from 'vue';\nexport default defineComponent({\n  setup() {\n    const formState = reactive({\n      ID: '',\n      title: '',\n      startTime: '',\n      endTime: '',\n      choices: [],\n      signature: '',\n      result: [],\n      type: false\n    });\n\n    let validateSignature = async (_rule, value) => {\n      if (value === '') {\n        return Promise.reject('请输入正确格式账户名');\n      } else if (value.length != value.split(\" \").join(\"\").length) {\n        return Promise.reject('输入包含空格');\n      } else {\n        return Promise.resolve();\n      }\n    };\n\n    const rules = {\n      signature: [{\n        required: true,\n        validator: validateSignature,\n        trigger: 'change'\n      }]\n    };\n    return {\n      formState,\n      rules,\n      validateSignature\n    };\n  },\n\n  created() {\n    emitter.on(\"info\", msg => {\n      console.log(\"11111\", msg);\n      this.formState.ID = msg.ID, this.formState.title = msg.title, this.formState.endTime = msg.ddl, this.formState.choices = msg.choice; // this.formState.type = true\n    });\n  },\n\n  methods: {\n    prevStep() {\n      this.$emit('prevStep');\n    },\n\n    nextStep() {\n      // for(var i of this.formState.result){\n      //   console.log(i)\n      // }\n      var res = [];\n\n      if (this.formState.signature !== '' && this.formState.signature.length == this.formState.signature.split(\" \").join(\"\").length && this.formState.pattern) {\n        res.push({\n          ID: this.formState.ID,\n          ballot: this.formState.pattern,\n          signature: this.formState.signature\n        });\n        console.log(\"构造数据\", res);\n        emitter.emit('upload', res);\n        this.$emit('nextStep');\n      }\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAkEA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AACA,eAAeD,eAAe,CAAC;EAC7BE,KAAK,GAAG;IACL,MAAMC,SAAQ,GAAIF,QA