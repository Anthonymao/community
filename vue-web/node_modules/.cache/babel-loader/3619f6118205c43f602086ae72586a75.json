{"ast":null,"code":"import emitter from '@/utils/bus';\nimport { message } from 'ant-design-vue';\nimport { defineComponent, reactive } from 'vue';\nconst columns = [{\n  title: 'ID',\n  dataIndex: '0',\n  ellipsis: true\n}, {\n  title: '加密选票',\n  dataIndex: '1',\n  ellipsis: true\n}, {\n  title: '选票签名',\n  dataIndex: '2',\n  ellipsis: true\n}];\nexport default defineComponent({\n  inject: ['reload'],\n\n  setup() {\n    const formState = reactive({\n      addr: '',\n      res: '',\n      sum: '',\n      data: [],\n      click: '',\n      result: []\n    });\n    return {\n      formState,\n      columns,\n      mypagination: {\n        defaultPageSize: 3\n      }\n    };\n  },\n\n  // created() {\n  //   emitter.on(\"resballot\", msg => {\n  //     // console.log(\"sdsdsdsdssssss\", msg.ballot)\n  //     this.formState.addr = msg.addr,\n  //       this.formState.data = msg.ballot\n  //   })\n  // },\n  created() {\n    console.log(\"chancan\", this.$route.params.info);\n    this.prevStep1(this.$route.params.info);\n  },\n\n  methods: {\n    prevStep1(msg) {\n      //  emitter.off('info')\n      msg = JSON.parse(msg);\n      this.formState.addr = msg.addr;\n      this.formState.data = msg.ballot;\n    },\n\n    prevStep() {\n      //  emitter.off('info')\n      // this.$emit('prevStep')\n      // this.reload()\n      this.$router.push('/result');\n    },\n\n    beforeUpload1(file) {\n      // console.log('上传前校验--文件类型', file)\n      // this.fileList = [file]\n      // console.log('选择了文件beforeUpload', this.fileList)\n      this.read1(file);\n      return false;\n    },\n\n    read1(f) {\n      const reader = new FileReader();\n      reader.readAsText(f, 'UTF-8');\n\n      reader.onload = fileReader => {\n        const fileData = fileReader.target.result;\n        var parafile = JSON.parse(fileData); // console.log(para, typeof (para));\n        // console.log(JSON.parse(reader.result));\n        // 上面的两个输出相同\n\n        var index = this.formState.addr.indexOf(parafile.addr);\n        console.log(\"indexshism\", index);\n\n        if (index == -1) {\n          message.destroy();\n          message.warning(\"文件上传错误，不是当前投票活动参数\");\n        } else {\n          var para = new Object();\n          para.P = parafile.P;\n          para.Q = parafile.Q;\n          para.G = parafile.G;\n          var arr = this.formState.data;\n          var n = this.formState.data.length;\n          var C = new Array(n);\n          var jsonC = new Array(n);\n          var Signs = new Array(n);\n          var ecdsa_pubs = new Array(n);\n\n          for (let i = 0; i < n; i++) {\n            console.log(arr[i][1]);\n            C[i] = JSON.parse(arr[i][3]);\n            jsonC[i] = JSON.stringify(C[i]);\n            Signs[i] = JSON.parse(arr[i][4]);\n            ecdsa_pubs[i] = JSON.parse(arr[i][1]);\n          }\n\n          var countC = count(para, C, Signs, ecdsa_pubs, jsonC, n);\n          console.log(\"CCCCCCCCCC\", countC);\n          console.log(\"CCCCCCCCCC\", ecdsa_pubs);\n          this.formState.share = decrypt0(para, countC.C1, parafile.sk);\n          this.formState.C1 = countC.C1;\n          this.formState.C2 = countC.C2;\n        }\n      };\n    },\n\n    hex2str(hex) {\n      var trimedStr = hex.trim();\n      var rawStr = trimedStr.substr(0, 2).toLowerCase() === \"0x\" ? trimedStr.substr(2) : trimedStr;\n      var len = rawStr.length;\n\n      if (len % 2 !== 0) {\n        alert(\"Illegal Format ASCII Code!\");\n        return \"\";\n      }\n\n      var curCharCode;\n      var resultStr = [];\n\n      for (var i = 0; i < len; i = i + 2) {\n        curCharCode = parseInt(rawStr.substr(i, 2), 16);\n        resultStr.push(String.fromCharCode(curCharCode));\n      }\n\n      return resultStr.join(\"\");\n    },\n\n    getres() {\n      this.formState.click = 'click';\n      this.$http.post('http://192.168.5.42:8888/voteravc/get_avc_result', {\n        name: sessionStorage.getItem('user'),\n        password: sessionStorage.getItem('pass'),\n        addr: this.formState.addr\n      }).then(response => {\n        console.log(response);\n\n        if (response.data.result == 'success') {\n          console.log(\"jieguo:\", response.data); // this.$emit('nextStep')\n\n          var para = new Object();\n          para.P = response.data.para[0];\n          para.Q = response.data.para[1];\n          para.G = response.data.para[2];\n          var arr = this.formState.data;\n          var k = response.data.para[4];\n          console.log(\"K\", k);\n          var CE = response.data.PK;\n          var n = this.formState.data.length;\n          var C = new Array(n);\n          var share = response.data.allshare; // var ecdsa_pubs = new Array(n)\n\n          for (let i = 0; i < n; i++) {\n            // console.log(arr[i][1])\n            C[i] = JSON.parse(arr[i][1]); // jsonC[i] = JSON.stringify(C[i])\n            //   ecdsa_pubs[i] = JSON.parse(arr[i][1])\n          }\n\n          var m = new Array();\n          console.log(\"dsdsdsdsdsds\", JSON.parse(share[0])); // var allshare = new Array()\n          // for(i in share){\n          //   allshare.push(JSON.parse(i))\n          // }\n\n          for (let j = 0; j < n; j++) {\n            var tmpx = new Array();\n            var tmpy = new Array();\n\n            for (let i = 0; i < k; i++) {\n              console.log(\"dsds\", share);\n              var tmpshare = JSON.parse(share[i]);\n              tmpx.push(tmpshare.x[j]);\n              tmpy.push(tmpshare.y[j]);\n            }\n\n            C[j].x = tmpx;\n            C[j].y = tmpy;\n            console.log(\"PK:\", CE[0]);\n            m[j] = decrypt11(para, C[j], CE[0], k); //console.log(C[j])\n            //console.log(decrypt11(para, C[j], CE, k))\n          }\n\n          console.log(m);\n\n          for (i in m) {\n            this.formState.score.push({\n              value: 0,\n              item: this.formState.choices[i],\n              key: Date.now()\n            });\n          } // var Cx = response.data.Cx\n          // var n = Cx.length\n          // var max = 100 //max为暴力破解上限\n          // var bsum = decrypt1(para, C2, Cx, n, max)\n          // console.log(\"b:\", bsum) //sum[m]\n          // this.formState.sum = bsum\n          // if (bsum > 0) {\n          //   this.formState.res = \"通过\"\n          // }\n          // if (bsum < 0) {\n          //   this.formState.res = \"未通过\"\n          // }\n          // if (bsum = 0) {\n          //   this.formState.res = \"中立\"\n          // }\n\n        }\n\n        if (response.data.result == 'error1') {\n          message.destroy();\n          message.warning('请检查登录');\n        }\n\n        if (response.data.result == 'wait') {\n          message.destroy();\n          message.warning('请等候所有的参与者共同恢复结果');\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAmEA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AAEA,MAAMC,OAAM,GAAI,CACd;EACEC,KAAK,EAAE,IADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CADc,EAMd;EACEF,KAAK,EAAE,MADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CANc,EAUX;EACDF,KAAK,EAAE,MADN;EAEDC,SAAS,EAAE,GAFV;EAGDC,QAAQ,EAAE;AAHT,CAVW,CAAhB;AAiBA,eAAeL,eAAe,CAAC;EAC7BM,MAAM,EAAE,CAAC,QAAD,CADqB;;EAE7BC,KAAK,GAAG;IAEN,MAAMC,SAAQ,GAAIP,QAAQ,CAAC;MACzBQ,IAAI,EAAE,EADmB;MAEzBC,GAAG,EAAE,EAFoB;MAGzBC,GAAG,EAAE,EAHoB;MAIzBC,IAAI,EAAE,EAJmB;MAKzBC,KAAK,EAAE,EALkB;MAMzBC,MAAM,EAAE;IANiB,CAAD,CAA1B;IASA,OAAO;MACLN,SADK;MAELN,OAFK;MAGLa,YAAY,EAAE;QACZC,eAAe,EAAE;MADL;IAHT,CAAP;EAOD,CApB4B;;EAqB7B;EACA;EACA;EACA;EACA;EAEA;EACA;EACAC,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuB,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,IAA1C;IACA,KAAKC,SAAL,CAAe,KAAKH,MAAL,CAAYC,MAAZ,CAAmBC,IAAlC;EACD,CAhC4B;;EAkC7BE,OAAO,EAAE;IACPD,SAAS,CAACE,GAAD,EAAM;MACb;MACAA,GAAE,GAAIC,IAAI,CAACC,KAAL,CAAWF,GAAX,CAAN;MACA,KAAKjB,SAAL,CAAeC,IAAf,GAAsBgB,GAAG,CAAChB,IAA1B;MACA,KAAKD,SAAL,CAAeI,IAAf,GAAsBa,GAAG,CAACG,MAA1B;IACD,CANM;;IAOPC,QAAQ,GAAG;MACT;MACA;MACA;MACA,KAAKC,OAAL,CAAaC,IAA