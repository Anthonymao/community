{"ast":null,"code":"import emitter from '@/utils/bus';\nimport { message } from 'ant-design-vue';\nimport { defineComponent, reactive } from 'vue';\nconst columns = [{\n  title: 'ID',\n  dataIndex: '0',\n  ellipsis: true\n}, {\n  title: '签名公钥',\n  dataIndex: '1',\n  ellipsis: true\n}, {\n  title: '权重',\n  dataIndex: '2'\n}, {\n  title: '选票',\n  dataIndex: '3',\n  ellipsis: true\n}, {\n  title: '签名',\n  dataIndex: '4',\n  ellipsis: true\n}];\nexport default defineComponent({\n  inject: ['reload'],\n\n  setup() {\n    const formStateWe = reactive({\n      addr: '',\n      title: '',\n      content: '',\n      endTime: '',\n      weight: '',\n      PKlist: [],\n      PK: '',\n      rawballot: '',\n      signature: '',\n      choice: '',\n      vote: false,\n      data: []\n    });\n\n    let validateSignature = async (_rule, value) => {\n      if (value === '') {\n        return Promise.reject('请根据要求操作');\n      } else {\n        return Promise.resolve();\n      }\n    };\n\n    const rules = {\n      signature: [{\n        required: true,\n        validator: validateSignature,\n        trigger: 'change'\n      }]\n    };\n    return {\n      rules,\n      validateSignature,\n      formStateWe,\n      columns\n    };\n  },\n\n  created() {\n    emitter.on(\"ballotshare\", msg => {\n      console.log(\"sdsdsdsdssssss\", msg);\n      this.formStateWe.addr = msg.addr, this.formStateWe.data = msg.ballot; //   this.formStateWe.title = msg.voteinfo[0],\n      //   this.formStateWe.content = msg.voteinfo[2],\n      //   this.formStateWe.endTime = msg.voteinfo[1],\n      //   this.formStateWe.weight = msg.weight,\n      //   this.formStateWe.PKlist = msg.PK,\n      //   this.formStateWe.vote = true\n    });\n  },\n\n  methods: {\n    beforeUpload(file) {\n      // console.log('上传前校验--文件类型', file)\n      // this.fileList = [file]\n      // console.log('选择了文件beforeUpload', this.fileList)\n      this.read(file);\n      return false;\n    },\n\n    read(f) {\n      const reader = new FileReader();\n      reader.readAsText(f, 'UTF-8');\n\n      reader.onload = fileReader => {\n        const fileData = fileReader.target.result;\n        var parafile = JSON.parse(fileData); // console.log(para, typeof (para));\n        // console.log(JSON.parse(reader.result));\n        // 上面的两个输出相同\n\n        var index = this.formStateWe.PKlist.indexOf(parafile.pk);\n        console.log(\"indexshism\", index);\n\n        if (index == -1) {\n          message.destroy();\n          message.warning(\"文件上传错误，不是当前投票活动参数\");\n        }\n\n        var para = new Object();\n        para.P = parafile.P;\n        para.Q = parafile.Q;\n        para.G = parafile.G;\n        var PK = generatepub(para, this.formStateWe.PKlist, this.formStateWe.PKlist.length);\n        console.log(\"系统公钥建立：\", PK);\n        this.formStateWe.PK = PK; // C[i] = encrypt(para, r, PK, m[i], 0)\n\n        var mi = this.formStateWe.choice * this.formStateWe.weight;\n        this.formStateWe.rawballot = JSON.stringify(encrypt(para, parafile.r, PK, mi, 0));\n        var ecdsa_pubs = new Object();\n        ecdsa_pubs.x = parafile.ecdsa_pk_x;\n        ecdsa_pubs.y = parafile.ecdsa_pk_y;\n        this.formStateWe.signature = sign(this.formStateWe.rawballot, parafile.ecdsa_sk, ecdsa_pubs); // signs[i] = sign(jsonC[i], man[i].ecdsa_sk, ecdsa_pubs[i])\n\n        console.log(\"选票和签名\", this.formStateWe.rawballot, this.formStateWe.signature);\n      };\n    },\n\n    prevStep() {\n      //  emitter.off('info')\n      this.$emit('prevStep');\n      this.reload();\n    },\n\n    nextStep() {\n      var res = [];\n\n      if (this.formState.signature !== '' && this.formState.signature.length == this.formState.signature.split(\" \").join(\"\").length && this.formState.pattern) {\n        res.push({\n          ID: this.formState.ID,\n          ballot: this.formState.pattern,\n          signature: this.formState.signature,\n          vote: 'avec'\n        });\n        console.log(\"构造数据\", res);\n        emitter.emit('upload', res);\n        this.$emit('nextStep');\n      } else if (this.formState.signature !== '' && this.formState.signature.length == this.formState.signature.split(\" \").join(\"\").length && this.formState.result.length) {\n        res.push({\n          ID: this.formState.ID,\n          ballot: this.formState.result,\n          signature: this.formState.signature,\n          vote: 'avec'\n        });\n        console.log(\"构造数据\", res);\n        emitter.emit('upload', res);\n        this.$emit('nextStep');\n      }\n    },\n\n    nextStepWe() {\n      console.log(this.formStateWe.PK); // this.$emit('nextStep')\n\n      if (this.formStateWe.choice) {\n        if (this.formStateWe.signature == '') {\n          message.destroy();\n          message.warning(\"请点击并上传本地加密信息文件\");\n        } // console.log(JSON.parse(this.formStateWe.rawballot))\n        // this.$emit('nextStep')\n\n\n        this.addweballot();\n      }\n    },\n\n    addweballot() {\n      this.$http.post('http://192.168.5.42:8888/voter/add_we_ballot', {\n        name: sessionStorage.getItem('user'),\n        password: sessionStorage.getItem('pass'),\n        uid: sessionStorage.getItem('uid'),\n        ballotstr: this.formStateWe.rawballot,\n        signature: JSON.stringify(this.formStateWe.signature),\n        weight: this.formStateWe.weight,\n        addr: this.formStateWe.addr\n      }).then(response => {\n        console.log(response);\n\n        if (response.data.result == 'success') {\n          emitter.emit('change', response.data);\n          console.log(\"jieguo:\", response);\n          emitter.emit('info1', response.data);\n          this.$emit('nextStep');\n        }\n\n        if (response.data.result == 'error1') {\n          message.destroy();\n          message.warning('请检查登录');\n        }\n\n        if (response.data.result == 'error2') {\n          message.destroy();\n          message.warning('没有找到对应投票项目');\n        }\n\n        if (response.data.result == 'error3') {\n          emitter.emit('change', response.data); // this.$emit('nextStep')\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAsFA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AAEA,MAAMC,OAAM,GAAI,CACd;EACEC,KAAK,EAAE,IADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CADc,EAMd;EACEF,KAAK,EAAE,MADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CANc,EAUX;EACDF,KAAK,EAAE,IADN;EAEDC,SAAS,EAAE;AAFV,CAVW,EAcd;EACED,KAAK,EAAE,IADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CAdc,EAmBd;EACEF,KAAK,EAAE,IADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CAnBc,CAAhB;AAyBA,eAAeL,eAAe,CAAC;EAC7BM,MAAM,EAAE,CAAC,QAAD,CADqB;;EAE7BC,KAAK,GAAG;IAEN,MAAMC,WAAU,GAAIP,QAAQ,CAAC;MAC3BQ,IAAI,EAAE,EADqB;MAE3BN,KAAK,EAAE,EAFoB;MAG3BO,OAAO,EAAE,EAHkB;MAI3BC,OAAO,EAAE,EAJkB;MAK3BC,MAAM,EAAE,EALmB;MAM3BC,MAAM,EAAE,EANmB;MAO3BC,EAAE,EAAE,EAPuB;MAQ3BC,SAAS,EAAE,EARgB;MAS3BC,SAAS,EAAE,EATgB;MAU3BC,MAAM,EAAE,EAVmB;MAW3BC,IAAI,EAAE,KAXqB;MAY3BC,IAAI,EAAE;IAZqB,CAAD,CAA5B;;IAcA,IAAIC,iBAAgB,GAAI,OAAOC,KAAP,EAAcC,KAAd,KAAwB;MAC9C,IAAIA,KAAI,KAAM,EAAd,EAAkB;QAChB,OAAOC,OAAO,CAACC,MAAR,CAAe,SAAf,CAAP;MACF,CAFA,MAGK;QACH,OAAOD,OAAO,CAACE,OAAR,EAAP;MACF;IACF,CAPA;;IAQA,MAAMC,KAAI,GAAI;MACZV,SAAS,EAAE,CAAC;QACVW,QAAQ,EAAE,IADA;QAEVC,SAAS,EAAER,iBAFD;QAGVS,OAAO,EAAE;MAHC,CAAD;IADC,CAAd;IAQA,OAAO;MACLH,KADK;MAELN,iBAFK;MAGLZ,WAHK;MAILN;IAJK,CAAP;EAMD,CAxC4B;;EAyC7B4B,OAAO,GAAG;IACRhC,OAAO,CAACiC,EAAR,CAAW,aAAX,EAA0BC,GAAE,IAAK;MAC/BC,OAAO,CAACC,GAAR,CAAY,gBAAZ,EAA8BF,GAA9B;MACA,KAAKxB,WAAL,CAAiBC,IAAjB,GAAwBuB,GAAG,CAACvB,IAA5B,EACE,KAAKD,WAAL,CAAiBW,IAAjB,GAAwBa,GAAG,CAACG,MAD9B,CAF+B,CAK/B;MACA;MACA;MACA;MACA;MACA;IAED,CAZD;EAaD,CAvD4B;;EAyD7BC,OAAO,EAAE;IACPC,YAAY,CAACC,IAAD,EAAO;MACjB;MACA;MACA;MACA,KAAKC,IAAL,CAAUD,IAAV;MACA,OAAO,KAAP;IACD,CAPM;;IAQPC,IAAI,CAACC,CAAD,EAAI;MACN,MAAMC,MAAK,GAAI,IAAIC,UAAJ,EAAf;MACAD,MAAM,CAACE,UAAP,CAAkBH,CAAlB,EAAqB,OAArB;;MACAC,MAAM,CAACG,MAAP,GAAgBC,UAAS,IAAK;QAC5B,MAAMC,QAAO,GAAID,UAAU,CAACE,MAAX,CAAkBC,MAAnC;QACA