',\r\n      title: '',\r\n      startTime: '',\r\n      endTime: '',\r\n      choices: [],\r\n      signature: '',\r\n      result: [],\r\n      type: '',\r\n      vote: false\r\n    });\r\n\r\n    const formStateWe = reactive({\r\n      ID: '',\r\n      title: '',\r\n      content: '',\r\n      startTime: '',\r\n      endTime: '',\r\n      signature: '',\r\n      choice: '',\r\n      vote: false\r\n    });\r\n    let validateSignature = async (_rule, value) => {\r\n      if (value === '') {\r\n        return Promise.reject('请输入正确格式账户名');\r\n      }\r\n      else if (value.length != value.split(\" \").join(\"\").length) {\r\n        return Promise.reject('输入包含空格');\r\n      }\r\n      else {\r\n        return Promise.resolve();\r\n      }\r\n    }\r\n    const rules = {\r\n      signature: [{\r\n        required: true,\r\n        validator: validateSignature,\r\n        trigger: 'change',\r\n      }],\r\n    };\r\n\r\n    return {\r\n      formState,\r\n      rules,\r\n      validateSignature,\r\n      formStateWe\r\n    };\r\n  },\r\n  mounted() {\r\n    emitter.on(\"info\", msg => {\r\n      if (msg.type == 'we') {\r\n        // this.formStateWe.ID = msg.ID,\r\n        // this.formStateWe.title = msg.title,\r\n        // this.formStateWe.content = msg.content,\r\n        // this.formStateWe.endTime = msg.ddl\r\n        this.formStateWe.vote = true\r\n      }\r\n      else {\r\n        this.formState.ID = msg.ID,\r\n          this.formState.title = msg.title,\r\n          this.formState.endTime = msg.ddl,\r\n          this.formState.choices = msg.choice,\r\n          this.formState.vote = true,\r\n          this.formState.type = msg.type\r\n        // this.formState.type = true\r\n      }\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    prevStep() {\r\n      //  emitter.off('info')\r\n      this.$emit('prevStep')\r\n      this.reload()\r\n    },\r\n    nextStep() {\r\n      var res = []\r\n      if (this.formState.signature !== '' && this.formState.signature.length == 