{"ast":null,"code":"import emitter from '@/utils/bus';\nimport { message } from 'ant-design-vue';\nimport { defineComponent, reactive } from 'vue';\nexport default defineComponent({\n  setup() {\n    const formState = reactive({\n      ID: '',\n      click: ''\n    });\n\n    let validateID = async (_rule, value) => {\n      if (value === '') {\n        return Promise.reject('请输入正确格式');\n      } else if (value.length != value.split(\" \").join(\"\").length) {\n        return Promise.reject('输入包含空格');\n      } else {\n        return Promise.resolve();\n      }\n    };\n\n    const rules = {\n      ID: [{\n        required: true,\n        validator: validateID,\n        trigger: 'change'\n      }]\n    };\n    return {\n      formState,\n      validateID,\n      rules\n    };\n  },\n\n  methods: {\n    nextStep() {\n      this.formState.click = 'click'; // this.$router.push({ name: 'addvotingavec', params: { id: 1, name: 'yizuodao' } })\n\n      if (this.formState.ID !== '' && this.formState.ID.length == this.formState.ID.split(\" \").join(\"\").length) {\n        console.log(this.formState.ID); // this.$emit('nextStep')\n\n        this.send();\n      }\n    },\n\n    send() {\n      this.$http.post('http://192.168.5.42:8888/voter/if_join_vote', {\n        name: sessionStorage.getItem('user'),\n        password: sessionStorage.getItem('pass'),\n        ID: this.formState.ID,\n        uid: sessionStorage.getItem('uid')\n      }).then(response => {\n        this.formState.click = '';\n        console.log(response);\n\n        if (response.data.result == 'exist') {\n          message.destroy();\n          message.warning('已成功加入该投票，请开始投票');\n        }\n\n        if (response.data.result == 'success') {\n          // console.log(\"jieguo:\",response)\n          if (response.data.type == 'we') {\n            this.$router.push({\n              name: 'addvotingwe',\n              params: {\n                info: JSON.stringify(response.data)\n              }\n            });\n          } else {\n            this.$router.push({\n              name: 'addvotingavec',\n              params: {\n                info: JSON.stringify(response.data)\n              }\n            });\n          } // this.$router.push({ name: 'addvotingavec', params: { info: response.data } })\n\n        }\n\n        if (response.data.result == 'error1') {\n          message.destroy();\n          message.warning('请检查登录');\n        }\n\n        if (response.data.result == 'error2') {\n          message.destroy();\n          message.warning('没有找到对应投票项目');\n        }\n\n        if (response.data.result == 'error3') {\n          message.destroy();\n          message.warning('没有该投票活动的投票权限！');\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAgCA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AACA,eAAeD,eAAe,CAAC;EAC7BE,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIF,QAAQ,CAAC;MACzBG,EAAE,EAAE,EADqB;MAEzBC,KAAK,EAAE;IAFkB,CAAD,CAA1B;;IAKA,IAAIC,UAAS,GAAI,OAAOC,KAAP,EAAcC,KAAd,KAAwB;MACvC,IAAIA,KAAI,KAAM,EAAd,EAAkB;QAChB,OAAOC,OAAO,CAACC,MAAR,CAAe,SAAf,CAAP;MACF,CAFA,MAGK,IAAIF,KAAK,CAACG,MAAN,IAAgBH,KAAK,CAACI,KAAN,CAAY,GAAZ,EAAiBC,IAAjB,CAAsB,EAAtB,EAA0BF,MAA9C,EAAsD;QACzD,OAAOF,OAAO,CAACC,MAAR,CAAe,QAAf,CAAP;MACF,CAFK,MAGA;QACH,OAAOD,OAAO,CAACK,OAAR,EAAP;MACF;IACD,CAVD;;IAYA,MAAMC,KAAI,GAAI;MACZX,EAAE,EAAE,CAAC;QACHY,QAAQ,EAAE,IADP;QAEHC,SAAS,EAAEX,UAFR;QAGHY,OAAO,EAAE;MAHN,CAAD;IADQ,CAAd;IAQA,OAAO;MACLf,SADK;MAELG,UAFK;MAGLS;IAHK,CAAP;EAKD,CAhC4B;;EAkC7BI,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,KAAKjB,SAAL,CAAeE,KAAf,GAAuB,OAAvB,CADS,CAET;;MACA,IAAI,KAAKF,SAAL,CAAeC,EAAf,KAAsB,EAAtB,IAA4B,KAAKD,SAAL,CAAeC,EAAf,CAAkBO,MAAlB,IAA4B,KAAKR,SAAL,CAAeC,EAAf,CAAkBQ,KAAlB,CAAwB,GAAxB,EAA6BC,IAA7B,CAAkC,EAAlC,EAAsCF,MAAlG,EAA0G;QACxGU,OAAO,CAACC,GAAR,CAAY,KAAKnB,SAAL,CAAeC,EAA3B,EADwG,CAExG;;QACA,KAAKmB,IAAL;MACF;IACD,CATM;;IAUPA,IAAI,GAAG;MACL,KAAKC,KAAL,CAAWC,IAAX,CAAgB,6CAAhB,EAA+D;QAC7DC,IAAI,EAAEC,cAAc,CAACC,OAAf,CAAuB,MAAvB,C