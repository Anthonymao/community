oad = fileReader => {\n        const fileData = fileReader.target.result;\n        var parafile = JSON.parse(fileData); // console.log(para, typeof (para));\n        // console.log(JSON.parse(reader.result));\n        // 上面的两个输出相同\n\n        var index = this.formState.addr.indexOf(parafile.addr);\n        console.log(\"indexshism\", index);\n\n        if (index == -1) {\n          message.destroy();\n          message.warning(\"文件上传错误，不是当前投票活动参数\");\n        }\n\n        var para = new Object();\n        para.P = parafile.P;\n        para.Q = parafile.Q;\n        para.G = parafile.G;\n        var arr = this.formState.data;\n        var n = this.formState.data.length;\n        var C = new Array(n);\n        var jsonC = new Array(n);\n        var Signs = new Array(n);\n        var ecdsa_pubs = new Array(n);\n\n        for (let i = 0; i < n; i++) {\n          console.log(arr[i][1]);\n          C[i] = JSON.parse(arr[i][3]);\n          jsonC[i] = JSON.stringify(C[i]);\n          Signs[i] = JSON.parse(arr[i][4]);\n          ecdsa_pubs[i] = JSON.parse(arr[i][1]);\n        }\n\n        var countC = count(para, C, Signs, ecdsa_pubs, jsonC, n);\n        console.log(\"CCCCCCCCCC\", countC);\n        console.log(\"CCCCCCCCCC\", ecdsa_pubs);\n        this.formState.share = decrypt0(para, countC.C1, parafile.sk);\n        this.formState.C1 = countC.C1;\n        this.formState.C2 = countC.C2;\n      };\n    },\n\n    prevStep() {\n      //  emitter.off('info')\n      this.$emit('prevStep');\n      this.reload();\n    },\n\n    nextStepWe() {\n      console.log(this.formState.share); // this.$emit('nextStep')\n\n      if (this.formState.share == '') {\n        message.destroy();\n        message.warning(\"请点击并上传本地加密信息文件\");\n      } // console.log(JSON.parse(this.formState.rawballot))\n      // this.$emit('nextStep')\n\n\n      this.addshare();\n    },\n\n    addshare() {\n      this.$http.post('http://192.168.5.42:8888/voterwe/upload_share', {\n        name: sessionStorage.getItem('user'),\n        password: sessionStorage.getItem('pass'),\n        uid: sessionStorage.getItem('uid'),\n        share: this.formState.share,\n        C1: this.formState.C1,\n        C2: this.formState.C2,\n        addr: this.formState.addr\n      }).then(response => {\n        console.log(response);\n\n        if (response.data.result == 'success') {\n          emitter.emit('change', response.data);\n          console.log(\"jieguo:\", response);\n          emitter.emit('info1', response.data);\n          this.$emit('nextStep');\n        }\n\n        if (response.data.result == 'error1') {\n          message.destroy();\n          message.warning('请检查登录');\n        }\n\n        if (response.data.result == 'error2') {\n          message.destroy();\n          message.warning('没有找到对应投票项目');\n        }\n\n        if (response.data.result == 'error3') {\n          emitter.emit('change', response.data); // this.$emit('nextStep')\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAuEA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AAEA,MAAMC,OAAM,GAAI,CACd;EACEC,KAAK,EAAE,IADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CADc,EAMd;EACEF,KAAK,EAAE,MADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CANc,EAUX;EACDF,KAAK,EAAE,IADN;EAEDC,SAAS,EAAE;AAFV,CAVW,EAcd;EACED,KAAK,EAAE,IADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CAdc,EAmBd;EACEF,KAAK,EAAE,IADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CAnBc,CAAhB;AAyBA,eAAeL,eAAe,CAAC;EAC7BM,MAAM,EAAE,CAAC,QAAD,CADqB;;EAE7BC,KAAK,GAAG;IAEN,MAAMC,SAAQ,GAAIP,QAAQ,CAAC;MACzBQ,IAAI,EAAE,EADmB;MAEzBC,IAAI,EAAE,KAFmB;MAGzBC,KAAK,EAAE,EAHkB;MAIzBC,EAAE,EAAE,EAJqB;MAKzBC,EAAE,EAAE,EALqB;MAMzBC,IAAI,EAAE;IANmB,CAAD,CAA1B;;IAQA,IAAIC,iBAAgB,GAAI,OAAOC,KAAP,EAAcC,KAAd,KAAwB;MAC9C,IAAIA,KAAI,KAAM,EAAd,EAAkB;QAChB,OAAOC,OAAO,CAACC,MAAR,CAAe,SAAf,CAAP;MACF,CAFA,MAGK;QACH,OAAOD,OAAO,CAACE,OAAR,EAAP;MACF;IACF,CAPA;;IAQA,MAAMC,KAAI,GAAI;MACZC,SAAS,EAAE,CAAC;QACVC,QAAQ,EAAE,IADA;QAEVC,SAA