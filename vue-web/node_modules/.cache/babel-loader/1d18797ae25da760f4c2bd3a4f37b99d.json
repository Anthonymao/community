{"ast":null,"code":"import dayjs from 'dayjs';\nimport { defineComponent, reactive } from 'vue';\nexport default defineComponent({\n  setup() {\n    const formState = reactive({\n      title: '',\n      date: undefined\n    });\n\n    const range = (start, end) => {\n      const result = [];\n\n      for (let i = start; i < end; i++) {\n        result.push(i);\n      }\n\n      return result;\n    };\n\n    const disabledDate = current => {\n      // Can not select days before today and today\n      return current && current < dayjs().endOf('day');\n    };\n\n    const disabledRangeTime = (_, type) => {\n      if (type === 'start') {\n        return {\n          disabledHours: () => range(0, 60).splice(4, 20),\n          disabledMinutes: () => range(30, 60),\n          disabledSeconds: () => [55, 56]\n        };\n      }\n\n      return {\n        disabledHours: () => range(0, 60).splice(20, 4),\n        disabledMinutes: () => range(0, 31),\n        disabledSeconds: () => [55, 56]\n      };\n    };\n\n    let validateID = async (_rule, value) => {\n      if (value === '') {\n        return Promise.reject('请输入正确格式投票主题');\n      } else if (value.length != value.split(\" \").join(\"\").length) {\n        return Promise.reject('输入包含空格');\n      } else {\n        return Promise.resolve();\n      }\n    };\n\n    const rules = {\n      title: [{\n        required: true,\n        validator: validateID,\n        trigger: 'change'\n      }],\n      date: [{\n        required: true,\n        message: '请选择投票日期'\n      }]\n    };\n    return {\n      dayjs,\n      formState,\n      validateID,\n      rules,\n      disabledDate,\n      disabledRangeTime\n    };\n  },\n\n  methods: {\n    nextStep() {\n      // if(this.formState.title !== '' && this.formState.title.length == this.formState.title.split(\" \").join(\"\").length){\n      //   console.log(this.formState.ID)\n      //   this.$emit('nextStep')\n      // }\n      // this.$emit('nextStep')\n      // console.log(this.formState.date[0])\n      if (this.formState.date) {\n        console.log(this.formState.date[0], this.formState.date[1]);\n        console.log(typeof this.formState.date[0]);\n        console.log(11111);\n        this.$emit('nextStep');\n      }\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAqDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAAyC,KAAzC;AACA,eAAeD,eAAe,CAAC;EAC7BE,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIF,QAAQ,CAAC;MACzBG,KAAK,EAAE,EADkB;MAEzBC,IAAI,EAAEC;IAFmB,CAAD,CAA1B;;IAIA,MAAMC,KAAI,GAAI,CAACC,KAAD,EAAQC,GAAR,KAAgB;MAC5B,MAAMC,MAAK,GAAI,EAAf;;MACA,KAAK,IAAIC,IAAIH,KAAb,EAAoBG,IAAIF,GAAxB,EAA6BE,CAAC,EAA9B,EAAkC;QAChCD,MAAM,CAACE,IAAP,CAAYD,CAAZ;MACF;;MACA,OAAOD,MAAP;IACD,CAND;;IAQA,MAAMG,YAAW,GAAIC,OAAM,IAAK;MAC9B;MACA,OAAOA,OAAM,IAAKA,OAAM,GAAIf,KAAK,GAAGgB,KAAR,CAAc,KAAd,CAA5B;IACD,CAHD;;IAKA,MAAMC,iBAAgB,GAAI,CAACC,CAAD,EAAIC,IAAJ,KAAa;MACrC,IAAIA,IAAG,KAAM,OAAb,EAAsB;QACpB,OAAO;UACLC,aAAa,EAAE,MAAMZ,KAAK,CAAC,CAAD,EAAI,EAAJ,CAAL,CAAaa,MAAb,CAAoB,CAApB,EAAuB,EAAvB,CADhB;UAELC,eAAe,EAAE,MAAMd,KAAK,CAAC,EAAD,EAAK,EAAL,CAFvB;UAGLe,eAAe,EAAE,MAAM,CAAC,EAAD,EAAK,EAAL;QAHlB,CAAP;MAKF;;MAEA,OAAO;QACLH,aAAa,EAAE,MAAMZ,KAAK,CAAC,CAAD,EAAI,EAAJ,CAAL,CAAaa,MAAb,CAAoB,EAApB,EAAwB,CAAxB,CADhB;QAELC,eAAe,EAAE,MAAMd,KAAK,CAAC,CAAD,EAAI,EAAJ,CAFvB;QAGLe,eAAe,EAAE,MAAM,CAAC,EAAD,EAAK,EAAL;MAHlB,CAAP;IAKD,CAdD;;IAiBA,IAAIC,UAAS,GAAI,OAAOC,KAAP,EAAcC,KAAd,KAAwB;MACvC,IAAIA,KAAI,KAAK,EAAb,EAAiB;QACf,OAAOC,OAAO,CAACC,MAAR,CAAe,aAAf,CAAP;MACF,CAFA,MAGK,IAAGF,KAAK,CAACG,MAAN,IAAgBH,KAAK,CAACI,KAAN,CAAY,GAAZ,EAAiBC,IAAjB,CAAsB,EAAtB,EAA0BF,MAA7C,EAAoD;QACvD,OAAOF,OAAO,CAACC,MAAR,CAAe,QAAf,CAAP;MACF,CAFK,MAGA;QACH,OAAOD,OAAO,CAACK,OAAR,EAAP;MACF;IACD,CAVD;;IAYD,MAAMC,KAAI,GAAI;MACX5B,KAAK,EAAE,CAAC;QACN6B,QAAQ,EAAE,IADJ;QAENC,SAAS,EAAEX,UAFL;QAGNY,OAAO,EAAE;MAHH,CAAD,CADI;MAOV9B,IAAI,EAAE,CACL;QACE4B,QAAQ,EAAE,IADZ;QAEEG,OAAO,EAAE;MAFX,CADK;IAPI,CAAd;IAeC,OAAO;MACLrC,KADK;MAELI,SAFK;MAGLoB,UAHK;MAILS,KAJK;MAKLnB,YALK;MAMLG;IANK,CAAP;EAQD,CAvE4B;;EAyE7BqB,OAAO,EAAC;IA