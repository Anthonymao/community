{"ast":null,"code":"import emitter from '@/utils/bus';\nimport { message } from 'ant-design-vue';\nimport { defineComponent, reactive } from 'vue';\nconst columns = [// {\n//   title: 'ID',\n//   dataIndex: '0',\n//   ellipsis: true,\n// },\n{\n  title: '问卷答案',\n  dataIndex: '1',\n  ellipsis: true\n}, {\n  title: '签名',\n  dataIndex: '2',\n  ellipsis: true\n}];\nexport default defineComponent({\n  inject: ['reload'],\n\n  setup() {\n    const formStateWe = reactive({\n      addr: '',\n      res: '',\n      sum: '',\n      data: []\n    });\n    return {\n      formStateWe,\n      columns,\n      mypagination: {\n        defaultPageSize: 3\n      }\n    };\n  },\n\n  created() {\n    console.log(\"chancan\", this.$route.params.info);\n    this.weresult(this.$route.params.info);\n  },\n\n  methods: {\n    weresult(msg) {\n      msg = JSON.parse(msg);\n      console.log(msg);\n      this.formStateWe.addr = msg.addr, this.formStateWe.data = msg.ballot;\n    },\n\n    beforeUpload1(file) {\n      // console.log('上传前校验--文件类型', file)\n      // this.fileList = [file]\n      // console.log('选择了文件beforeUpload', this.fileList)\n      this.read1(file);\n      return false;\n    },\n\n    read1(f) {\n      const reader = new FileReader();\n      reader.readAsText(f, 'UTF-8');\n\n      reader.onload = fileReader => {\n        const fileData = fileReader.target.result;\n        var parafile = JSON.parse(fileData); // console.log(para, typeof (para));\n        // console.log(JSON.parse(reader.result));\n        // 上面的两个输出相同\n\n        var index = this.formStateWe.addr.indexOf(parafile.addr);\n        console.log(\"indexshism\", index);\n\n        if (index == -1) {\n          message.destroy();\n          message.warning(\"文件上传错误，不是当前投票活动参数\");\n        }\n\n        var para = new Object();\n        para.P = parafile.P;\n        para.Q = parafile.Q;\n        para.G = parafile.G;\n        var arr = this.formStateWe.data;\n        var n = this.formStateWe.data.length;\n        var C = new Array(n);\n        var jsonC = new Array(n);\n        var Signs = new Array(n);\n        var ecdsa_pubs = new Array(n);\n\n        for (let i = 0; i < n; i++) {\n          console.log(arr[i][1]);\n          C[i] = JSON.parse(arr[i][3]);\n          jsonC[i] = JSON.stringify(C[i]);\n          Signs[i] = JSON.parse(arr[i][4]);\n          ecdsa_pubs[i] = JSON.parse(arr[i][1]);\n        }\n\n        var countC = count(para, C, Signs, ecdsa_pubs, jsonC, n);\n        console.log(\"CCCCCCCCCC\", countC);\n        console.log(\"CCCCCCCCCC\", ecdsa_pubs);\n        this.formStateWe.share = decrypt0(para, countC.C1, parafile.sk);\n        this.formStateWe.C1 = countC.C1;\n        this.formStateWe.C2 = countC.C2;\n      };\n    },\n\n    prevStep() {\n      //  emitter.off('info')\n      // this.$emit('prevStep')\n      this.$router.push('/result');\n    },\n\n    getres() {\n      this.$http.post('http://192.168.5.42:8888/voter/get_we_result', {\n        name: sessionStorage.getItem('user'),\n        password: sessionStorage.getItem('pass'),\n        addr: this.formStateWe.addr\n      }).then(response => {\n        console.log(response);\n\n        if (response.data.result == 'success') {\n          console.log(\"jieguo:\", response.data); // this.$emit('nextStep')\n\n          var para = new Object();\n          para.P = response.data.para[0];\n          para.Q = response.data.para[1];\n          para.G = response.data.para[2];\n          var C2 = response.data.C2;\n          var Cx = response.data.Cx;\n          var n = Cx.length;\n          var max = 100; //max为暴力破解上限\n\n          var bsum = decrypt1(para, C2, Cx, n, max);\n          console.log(\"b:\", bsum); //sum[m]\n\n          this.formStateWe.sum = bsum;\n\n          if (bsum > 0) {\n            this.formStateWe.res = \"通过\";\n          }\n\n          if (bsum < 0) {\n            this.formStateWe.res = \"未通过\";\n          }\n\n          if (bsum = 0) {\n            this.formStateWe.res = \"中立\";\n          }\n        }\n\n        if (response.data.result == 'error1') {\n          message.destroy();\n          message.warning('请检查登录');\n        }\n\n        if (response.data.result == 'wait') {\n          message.destroy();\n          message.warning('请等候所有的参与者共同恢复结果');\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAgEA,OAAOA,OAAP,MAAoB,aAApB;AACA,SAASC,OAAT,QAAwB,gBAAxB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAA0C,KAA1C;AAEA,MAAMC,OAAM,GAAI,CACd;AACA;AACA;AACA;AACA;AACA;EACEC,KAAK,EAAE,MADT;EAEEC,SAAS,EAAE,GAFb;EAGEC,QAAQ,EAAE;AAHZ,CANc,EAUX;EACDF,KAAK,EAAE,IADN;EAEDC,SAAS,EAAE,GAFV;EAGDC,QAAQ,EAAE;AAHT,CAVW,CAAhB;AAgBA,eAAeL,eAAe,CAAC;EAC7BM,MAAM,EAAE,CAAC,QAAD,CADqB;;EAE7BC,KAAK,GAAG;IAEN,MAAMC,WAAU,GAAIP,QAAQ,CAAC;MAC3BQ,IAAI,EAAE,EADqB;MAE3BC,GAAG,EAAE,EAFsB;MAG3BC,GAAG,EAAE,EAHsB;MAI3BC,IAAI,EAAE;IAJqB,CAAD,CAA5B;IAOA,OAAO;MACLJ,WADK;MAELN,OAFK;MAGLW,YAAY,EAAE;QACZC,eAAe,EAAE;MADL;IAHT,CAAP;EAOD,CAlB4B;;EAmB7BC,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuB,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,IAA1C;IACA,KAAKC,QAAL,CAAc,KAAKH,MAAL,CAAYC,MAAZ,CAAmBC,IAAjC;EACD,CAtB4B;;EAwB7BE,OAAO,EAAE;IACPD,QAAQ,CAACE,GAAD,EAAM;MACZA,GAAE,GAAIC,IAAI,CAACC,KAAL,CAAWF,GAAX,CAAN;MACAP,OAAO,CAACC,GAAR,CAAYM,GAAZ;MACA,KAAKf,WAAL,CAAiBC,IAAjB,GAAwBc,GAAG,CAACd,IAA5B,EACE,KAAKD,WAAL,CAAiBI,IAAjB,GAAwBW,GAAG,CAACG,MAD9B;IAED,CANM;;IAQPC,aAAa,CAACC,IAAD,EAAO;MAClB;MACA;MACA;MACA,KAAKC,KAAL,CAAWD,IAAX;MACA,OAAO,KAAP;IACD,CAdM;;IAePC,KAAK,CAACC,CAAD,EAAI;MACP,MAAMC,MAAK,GAAI,IAAIC,UAAJ,EAAf;MACAD,MAAM,CAACE,UAAP,CAAkBH,CAAlB,EAAqB,OAArB;;MACAC,MAAM,CAACG,MAAP,GAAgBC,UAAS,IAAK;QAC5B,MAAMC,QAAO,GAAID,UAAU,CAACE,MAAX,CAAkBC,MAAnC;QACA,IAAIC,QAAO,GAAIf,IAAI,CAACC,KAAL,CAAWW,QAAX,CAAf,CAF4B,CAG5B;QACA;QACA;;QACA,IAAII,KAAI,GAAI,KAAKhC,WAAL,CAAiBC,IAAjB,CAAsBgC,OAAtB,CAA8BF,QAAQ,CAAC9B,IAAvC,CAAZ;QACAO,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BuB,KAA1B;;QACA,IAAIA,KAAI,IAAK,CAAC,CAAd,EAAiB;UACfzC,OAAO,CAAC2C,OAAR;UACA3C,OAAO,CAAC4C,OAAR,CAAgB,mBAAhB;QACF;;QAEA,IAAIC,IAAG,GAAI,IAAIC,MAAJ,EAAX;QACAD,IAAI,