n      addDomain,\n      removeDomain\n    };\n  },\n\n  methods: {\n    nextStep() {\n      // if(this.formState.title !== '' && this.formState.title.length == this.formState.title.split(\" \").join(\"\").length){\n      //   console.log(this.formState.ID)\n      //   this.$emit('nextStep')\n      // }\n      // this.$emit('nextStep')\n      // console.log(this.formState.date[0])\n      if (this.formState.date) {\n        console.log(this.formState.date[0], this.formState.date[1]);\n        console.log(typeof this.formState.date[0]);\n        console.log(11111);\n        this.$emit('nextStep');\n      }\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAqFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,eAAT,EAA0BC,QAA1B,QAAyC,KAAzC;AACA,eAAeD,eAAe,CAAC;EAC7BE,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIF,QAAQ,CAAC;MACzBG,KAAK,EAAE,EADkB;MAEzBC,IAAI,EAAEC,SAFmB;MAGzBC,OAAO,EAAE;IAHgB,CAAD,CAA1B;;IAMA,MAAMC,KAAI,GAAI,CAACC,KAAD,EAAQC,GAAR,KAAgB;MAC5B,MAAMC,MAAK,GAAI,EAAf;;MACA,KAAK,IAAIC,IAAIH,KAAb,EAAoBG,IAAIF,GAAxB,EAA6BE,CAAC,EAA9B,EAAkC;QAChCD,MAAM,CAACE,IAAP,CAAYD,CAAZ;MACF;;MACA,OAAOD,MAAP;IACD,CAND;;IAOA,MAAMG,YAAW,GAAIC,OAAM,IAAK;MAC9B;MACA,OAAOA,OAAM,IAAKA,OAAM,GAAIhB,KAAK,GAAGiB,KAAR,CAAc,KAAd,CAA5B;IACD,CAHD;;IAIA,MAAMC,iBAAgB,GAAI,CAACC,CAAD,EAAIC,IAAJ,KAAa;MACrC,IAAIA,IAAG,KAAM,OAAb,EAAsB;QACpB,OAAO;UACLC,aAAa,EAAE,MAAMZ,KAAK,CAAC,CAAD,EAAI,EAAJ,CAAL,CAAaa,MAAb,CAAoB,CAApB,EAAuB,EAAvB,CADhB;UAELC,eAAe,EAAE,MAAMd,KAAK,CAAC,EAAD,EAAK,EAAL,CAFvB;UAGLe,eAAe,EAAE,MAAM,CAAC,EAAD,EAAK,EAAL;QAHlB,CAAP;MAKF;;MAEA,OAAO;QACLH,aAAa,EAAE,MAAMZ,KAAK,CAAC,CAAD,EAAI,EAAJ,CAAL,CAAaa,MAAb,CAAoB,EAApB,EAAwB,CAAxB,CADhB;QAELC,eAAe,EAAE,MAAMd,KAAK,CAAC,CAAD,EAAI,EAAJ,CAFvB;QAGLe,eAAe,EAAE,MAAM,CAAC,EAAD,EAAK,EAAL;MAHlB,CAAP;IAKD,CAdD;;IAgBF,MAAMC,YAAW,GAAIC,IAAG,IAAK;MACzB,IAAIC,KAAI,GAAIvB,SAAS,CAACI,OAAV,CAAkBoB,OAAlB,CAA0BF,IAA1B,CAAZ;;MAEA,IAAIC,KAAI,KAAM,CAAC,CAAf,EAAkB;QAChBvB,SAAS,CAACI,OAAV,CAAkBc,MAAlB,CAAyBK,KAAzB,EAAgC,CAAhC;MACF;IACD,CANH;;IAQE,MAAME,SAAQ,GAAI,MAAM;MACtBzB,SAAS,CAACI,OA